{
    "extends": ["typestrict", "tslint-config-prettier"],
    "rules": {
        "adjacent-overload-signatures": true,
        "no-namespace": true,
        "no-reference": true,
        "no-duplicate-switch-case": true,
        "no-shadowed-variable": true,
        "no-implicit-dependencies": true,
        "no-console": true
    }
}
